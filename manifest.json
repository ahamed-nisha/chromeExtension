{
    "manifest_version": 3,
    "name": "One Calendar",
    "description": "Adds user's calendars as an extension providing easy access",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "./assets/calendar-icon.png"
    },
    "icons": {
      "16": "./assets/calendar-icon.png",
      "32": "./assets/calendar-icon.png",
      "48": "./assets/calendar-icon.png",
      "128": "./assets/calendar-icon.png"
    },
    "author": "Nisha & Jose",
    "permissions": ["storage", "webNavigation", "tabs"]
  }
  